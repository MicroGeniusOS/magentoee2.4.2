"CREATE TABLE `purchase_order_rule` (\n  `rule_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Rule ID',\n  `name` varchar(255) NOT NULL COMMENT 'Rule Name',\n  `description` text DEFAULT NULL COMMENT 'Rule Description',\n  `is_active` tinyint(1) NOT NULL COMMENT 'Rule Status',\n  `applies_to_all` tinyint(1) NOT NULL COMMENT 'Does this rule apply to all roles?',\n  `company_id` int(10) unsigned NOT NULL COMMENT 'Company ID',\n  `conditions_serialized` mediumtext DEFAULT NULL COMMENT 'Serialized conditions for rule',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Updated At',\n  `created_by` int(10) unsigned DEFAULT NULL COMMENT 'Customer ID',\n  PRIMARY KEY (`rule_id`),\n  UNIQUE KEY `PURCHASE_ORDER_RULE_COMPANY_ID_NAME` (`company_id`,`name`),\n  KEY `PURCHASE_ORDER_RULE_CREATED_BY_CUSTOMER_ENTITY_ENTITY_ID` (`created_by`),\n  CONSTRAINT `PURCHASE_ORDER_RULE_COMPANY_ID_COMPANY_ENTITY_ID` FOREIGN KEY (`company_id`) REFERENCES `company` (`entity_id`) ON DELETE CASCADE,\n  CONSTRAINT `PURCHASE_ORDER_RULE_CREATED_BY_CUSTOMER_ENTITY_ENTITY_ID` FOREIGN KEY (`created_by`) REFERENCES `customer_entity` (`entity_id`) ON DELETE SET NULL\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Purchase Order Approval Rules Table'"