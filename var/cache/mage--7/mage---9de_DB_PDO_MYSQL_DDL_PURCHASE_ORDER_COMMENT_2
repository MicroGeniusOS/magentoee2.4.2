"CREATE TABLE `purchase_order_comment` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `purchase_order_id` int(10) unsigned NOT NULL COMMENT 'Purchase Order Id',\n  `creator_id` int(10) unsigned NOT NULL COMMENT 'Comment author ID',\n  `comment` text NOT NULL COMMENT 'Comment',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  PRIMARY KEY (`entity_id`),\n  KEY `PURCHASE_ORDER_COMMENT_PURCHASE_ORDER_ID` (`purchase_order_id`),\n  KEY `PURCHASE_ORDER_COMMENT_CREATED_AT` (`created_at`),\n  CONSTRAINT `PURCHASE_ORDER_COMMENT_PURCHASE_ORDER_ID_PURCHASE_ORDER_ENTT_ID` FOREIGN KEY (`purchase_order_id`) REFERENCES `purchase_order` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='PO Request Comments Table'"