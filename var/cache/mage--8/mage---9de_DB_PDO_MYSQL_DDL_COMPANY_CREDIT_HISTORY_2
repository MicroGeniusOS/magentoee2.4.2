"CREATE TABLE `company_credit_history` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Credit ID',\n  `company_credit_id` int(10) unsigned NOT NULL COMMENT 'Company Credit ID',\n  `user_id` int(10) unsigned DEFAULT NULL COMMENT 'User Id',\n  `user_type` int(11) NOT NULL DEFAULT 0 COMMENT 'User Type',\n  `currency_credit` varchar(3) NOT NULL COMMENT 'Currency Code Credit',\n  `currency_operation` varchar(3) NOT NULL COMMENT 'Currency Code Operation',\n  `rate` decimal(24,12) NOT NULL DEFAULT 0.000000000000 COMMENT 'Currency Rate',\n  `rate_credit` decimal(24,12) DEFAULT 0.000000000000 COMMENT 'Credit Currency Rate',\n  `amount` decimal(20,4) NOT NULL DEFAULT 0.0000 COMMENT 'Amount',\n  `balance` decimal(20,4) NOT NULL DEFAULT 0.0000 COMMENT 'Balance',\n  `credit_limit` decimal(20,4) NOT NULL DEFAULT 0.0000 COMMENT 'Credit Limit',\n  `available_credit` decimal(20,4) NOT NULL DEFAULT 0.0000 COMMENT 'Available Credit',\n  `type` int(11) NOT NULL DEFAULT 0 COMMENT 'Type',\n  `datetime` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Datetime',\n  `purchase_order` varchar(60) DEFAULT NULL COMMENT 'Purchase order number (deprecated; use custom_reference_number)',\n  `custom_reference_number` varchar(60) DEFAULT NULL COMMENT 'Custom reference number',\n  `comment` text NOT NULL COMMENT 'Comment',\n  PRIMARY KEY (`entity_id`),\n  KEY `COMPANY_CREDIT_HISTORY_COMPANY_CREDIT_ID` (`company_credit_id`),\n  CONSTRAINT `COMPANY_CREDIT_HISTORY_COMPANY_CREDIT_ID_COMPANY_CREDIT_ENTT_ID` FOREIGN KEY (`company_credit_id`) REFERENCES `company_credit` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Company Credit History Table'"