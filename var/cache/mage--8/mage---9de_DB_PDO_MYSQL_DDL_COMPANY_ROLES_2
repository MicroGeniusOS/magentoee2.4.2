"CREATE TABLE `company_roles` (\n  `role_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Primary Role ID',\n  `sort_order` int(10) unsigned NOT NULL COMMENT 'Sorting order',\n  `role_name` varchar(40) DEFAULT NULL COMMENT 'Company role name',\n  `company_id` int(10) unsigned NOT NULL COMMENT 'Company ID',\n  PRIMARY KEY (`role_id`),\n  KEY `COMPANY_ROLES_COMPANY_ID` (`company_id`),\n  CONSTRAINT `COMPANY_ROLES_COMPANY_ID_COMPANY_ENTITY_ID` FOREIGN KEY (`company_id`) REFERENCES `company` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='Roles Table'"