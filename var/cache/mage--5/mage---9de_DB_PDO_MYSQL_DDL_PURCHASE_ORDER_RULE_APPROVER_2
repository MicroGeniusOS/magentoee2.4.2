"CREATE TABLE `purchase_order_rule_approver` (\n  `rule_id` int(10) unsigned NOT NULL COMMENT 'Purchase Order Rule ID',\n  `role_id` int(10) unsigned DEFAULT NULL COMMENT 'Company Role ID',\n  `requires_admin_approval` tinyint(1) DEFAULT NULL COMMENT 'Admin Approval Required',\n  `requires_manager_approval` tinyint(1) DEFAULT NULL COMMENT 'Manager Approval Required',\n  UNIQUE KEY `PURCHASE_ORDER_RULE_APPROVER_RULE_ID_ROLE_ID` (`rule_id`,`role_id`),\n  KEY `PURCHASE_ORDER_RULE_APPROVER_ROLE_ID_COMPANY_ROLES_ROLE_ID` (`role_id`),\n  CONSTRAINT `PURCHASE_ORDER_RULE_APPROVER_ROLE_ID_COMPANY_ROLES_ROLE_ID` FOREIGN KEY (`role_id`) REFERENCES `company_roles` (`role_id`) ON DELETE CASCADE,\n  CONSTRAINT `PURCHASE_ORDER_RULE_APPROVER_RULE_ID_PURCHASE_ORDER_RULE_RULE_ID` FOREIGN KEY (`rule_id`) REFERENCES `purchase_order_rule` (`rule_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Purchase Order Approval Rules Approvers'"