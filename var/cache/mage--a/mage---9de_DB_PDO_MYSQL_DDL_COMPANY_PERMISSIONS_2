"CREATE TABLE `company_permissions` (\n  `permission_id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Primary Permission ID',\n  `role_id` int(10) unsigned NOT NULL COMMENT 'Role ID',\n  `resource_id` varchar(80) DEFAULT NULL COMMENT 'Resource ID',\n  `permission` varchar(80) DEFAULT NULL COMMENT 'Permission',\n  PRIMARY KEY (`permission_id`),\n  KEY `COMPANY_PERMISSIONS_ROLE_ID` (`role_id`),\n  CONSTRAINT `COMPANY_PERMISSIONS_ROLE_ID_COMPANY_ROLES_ROLE_ID` FOREIGN KEY (`role_id`) REFERENCES `company_roles` (`role_id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=69 DEFAULT CHARSET=utf8 COMMENT='Permissions Table'"