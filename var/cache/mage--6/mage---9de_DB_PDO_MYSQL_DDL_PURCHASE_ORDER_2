"CREATE TABLE `purchase_order` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity ID',\n  `increment_id` varchar(50) DEFAULT NULL COMMENT 'Purchase Order Increment Id',\n  `quote_id` int(10) unsigned NOT NULL COMMENT 'Quote ID',\n  `status` varchar(255) NOT NULL COMMENT 'Purchase Order Request status',\n  `creator_id` int(10) unsigned DEFAULT NULL COMMENT 'Purchase Order Request creator id',\n  `company_id` int(10) unsigned NOT NULL COMMENT 'Company ID',\n  `address_id` int(10) unsigned NOT NULL COMMENT 'Address ID',\n  `shipping_method` varchar(40) DEFAULT NULL COMMENT 'Shipping Method',\n  `payment_method` varchar(255) DEFAULT NULL COMMENT 'Payment Method',\n  `grand_total` decimal(12,4) NOT NULL COMMENT 'PO Grand Total',\n  `snapshot` mediumtext DEFAULT NULL COMMENT 'Associated Quote Snapshot',\n  `is_validate` smallint(6) NOT NULL DEFAULT 0 COMMENT 'Is Validate',\n  `order_id` int(10) unsigned DEFAULT NULL COMMENT 'Order Reference ID',\n  `order_increment_id` varchar(50) DEFAULT NULL COMMENT 'Order Reference Increment ID',\n  `auto_approved` tinyint(1) DEFAULT 0 COMMENT 'Was the order auto approved?',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Updated At',\n  PRIMARY KEY (`entity_id`),\n  UNIQUE KEY `PURCHASE_ORDER_QUOTE_ID` (`quote_id`),\n  KEY `PURCHASE_ORDER_COMPANY_ID_COMPANY_ENTITY_ID` (`company_id`),\n  KEY `PURCHASE_ORDER_CREATOR_ID` (`creator_id`),\n  CONSTRAINT `PURCHASE_ORDER_COMPANY_ID_COMPANY_ENTITY_ID` FOREIGN KEY (`company_id`) REFERENCES `company` (`entity_id`) ON DELETE CASCADE,\n  CONSTRAINT `PURCHASE_ORDER_CREATOR_ID_CUSTOMER_ENTITY_ENTITY_ID` FOREIGN KEY (`creator_id`) REFERENCES `customer_entity` (`entity_id`) ON DELETE SET NULL\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='purchase_order'"